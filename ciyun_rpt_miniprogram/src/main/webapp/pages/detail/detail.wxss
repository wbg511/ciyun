}
.new-report-other {
    color: #000;
    padding: 5px 0px;
    text-indent: 10px;
}
.new-report-other text {
    color: #666;
}
.report-name {
    color: #000;
    text-align: left;
    padding: 0 5px;
    width: 100px;
    padding-top: 10px;
    border-bottom: 1px solid #E0E0E2;
    vertical-align: top;
    flex: 0.86;
}
.flex{
  display: flex;
}
.report-value {
  flex: 2.14;
}
.report-value .p{
    padding: 5px;
    line-height: 24px;
    text-indent: 0;
}
/* 航天汇总分析专有样式  */
.app-check-group .app-check-list {
    float: left;
    padding: 5px 0;
}
.app-check-box {
    padding: 5px;
    display: inline-block;
    min-height: 24px;
    position: relative;
}
.app-check {
    position: absolute;
    width: 20px;
    height: 20px;
}
.checkbox {
    display: inline-block;
    width: 20px;
    height: 20px;
    position: relative;
    overflow: visible;
    border: 0;
    background: 0 0;
    -webkit-appearance: none;
    vertical-align: middle;
    border-radius: 2px;
    box-sizing: border-box;
}
.checkbox:checked:disabled:before {
    background-color: #F44336;
}
.checkbox.disabled{
    opacity: 0.4;
    background-color: #eee;
}
.checkbox.checked::after{
    color: #fff;
    background: #F44336;
    position: absolute;
    left: 0px;
    top: 0px;
    z-index: 3;
    content: '√';
    font-size: 15px;
    display: block;
    width: 20px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    border-radius: 2px;
}
.checkbox:before {
    position: absolute;
    left: 0px;
    top: 0px;
    z-index: 2;
    content: '';
    display: block;
    width: 20px;
    height: 20px;
    text-align: center;
    line-height: 20px;
    border: 1px solid #dfe0e1;
    border-radius: 2px;
    box-sizing:border-box;
}
.checkbox:checked:disabled:after {
    color: #fff;
    background: #F44336;
}
.checkbox:checked:after {
    position: absolute;
    left: 0px;
    top: 0px;
    z-index: 3;
    content: '√';
    font-size: 15px;
    display: block;
    width: 20px;
    height: 20px;
    line-height: 20px;
    text-align: center;
    background: #F44336;
    color: #fff;
    border-radius: 2px;
}
.checkbox:disabled:after {
    color: #aaa;
    background: #ddd;
}

.app-check-item {
    padding-left: 22px;
}

