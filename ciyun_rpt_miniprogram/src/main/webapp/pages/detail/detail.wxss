/* pages/detail/detail.wxss */
.navbar{
background-color: #fff;
    height: 100rpx;
    line-height: 100rpx;
    position: fixed; 
    z-index: 99;
    top: 0px;
    width: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    text-align: center;
}
.navbar .item.active {
    border-bottom: 2px solid #6fba2c;

}
.navbar .item.active text{
    color: #6fba2c;
    font-weight: bold;
}
.navbar text{
      display: block;
    text-align: center;
    color: #383838;
    line-height: 19rpx;
    height: 65rpx;
    border-right: 1rpx solid #dadada;
    font-size: 14px;
}
.navbar .item{
    -webkit-box-flex: 1;
