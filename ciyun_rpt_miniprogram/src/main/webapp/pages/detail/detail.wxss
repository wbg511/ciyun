/* 基本信息*/
.report-content {
    padding-top: 98rpx;
    padding-bottom: 98rpx;
}
.report-content:last-child {
    padding-top: 150rpx;
    padding-bottom: 98rpx;
}
.form-view {
    padding: 0 10px;
}
.form-view .form-group {
    border-bottom: 0.5px solid #dbdbdb;
    background-color: #fff;
    margin-right: -10px;
    margin-left: -10px;
    height: 120rpx;
    display:flex;
    align-items:center;
    justify-content:center;

}
.form-view .form-group .form-label {
    float: left;
    text-align: left;
    width: 250rpx;
    color: #888888;
}
.form-group .form-label, .form-group .form-val {
    position: relative;
    z-index: 2;
    min-height: 1px;
    padding-right: 10px;
    padding-left: 15px;
    float: left;
    width:500rpx;
    text-align:left;
}
.text-l{
  text-align:left;
}
.form-view .form-group .form-val view{
    color: #222;
}
.form .form-group:before,.form .form-group:after {
    display: table;
    content: " ";
}
.form .form-group:after {
    clear: both;
}
.infomation-footer{
  margin-top: 90px;
  font-size: 12px;
  color: #888;
  text-align: center;
}
/* 汇总分析 */
