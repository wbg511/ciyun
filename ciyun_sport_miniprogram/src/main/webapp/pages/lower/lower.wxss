/*  pages/register/register.wxss */
page{
  background:#f3f5f6;
}
.container{
  padding:30rpx;
  background:#fff;
  padding-bottom: 0
}
.box_title{
  width:100%;
  height:200rpx;
}
.box_title image{
  width:120rpx;
  height:120rpx;
  border-radius: 50%;
  margin-top:40rpx;
  margin-left: 40%
}
.login{
  margin-top:40rpx;
}
.container_list{
  width:690rpx;
  height:100rpx;
  border-bottom: 1rpx solid #e5e5e5;
  background:#fff;
  position: relative;
}
.container_list:last-child{
  border:0;
}
.container_left{
  display: block;
  float: left;
  width:30%;
  height:100%;
  line-height:100rpx; 
  text-align: left;
  font-size:30rpx;
  color:#666666 
}
.container_right{
  display: block;
  float: right;
  width:68%;
  height:100%;
  line-height: 100rpx;
}
.container_right input{
  display: bloak;
  float: left;
  width: 98%;
  height:100rpx;
  padding-left:2%;
  font-size:30rpx;
  color:#222222;
}
.container_list image{
  width:20rpx;
  height:36rpx;
  position: absolute;
  right:0;
  top:40rpx;
}